"{\n    \"Comment\": \"Operations data extraction process\",\n    \"StartAt\": \"EXECUTE_OPS_EXTRACT\",\n    \"States\": {\n        \"EXECUTE_OPS_EXTRACT\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::glue:startJobRun.sync\",\n            \"ResultPath\": \"$.OPS_EXTRACT_RESULT\",\n            \"Parameters\": {\n                \"JobName\": \"job_odp_load_status_extractor\",\n                \"Arguments\": {\n                    \"--max_threads\": \"16\"\n                }\n            },\n            \"Catch\": [\n                {\n                    \"ErrorEquals\": [\n                        \"States.TaskFailed\"\n                    ],\n                    \"Next\": \"LOAD_STATUS_EXTRACTION_FAILURE\"\n                }\n            ],\n            \"Next\": \"REFRESH SPOTFIRE JOB LOAD STATUS\"\n        },\n        \"REFRESH SPOTFIRE JOB LOAD STATUS\": {\n            \"Type\": \"Task\",\n            \"InputPath\": \"$\",\n            \"ResultPath\": \"$\",\n            \"Parameters\": {\n                \"REPORT_LOCATION\": \"/Service Data Analytics/Job Monitoring/Analysis/ODP Service Suite Job Load Status\"\n            },\n            \"Resource\": \"arn:aws:lambda:us-east-1:245792935030:function:odp-spotfire-refresh\",\n            \"Catch\": [\n                {\n                    \"ErrorEquals\": [\n                        \"States.ALL\"\n                    ],\n                    \"Next\": \"SPOTFIRE_REFRESH_FAILURE\"\n                }\n            ],\n            \"End\": true\n        },\n        \"LOAD_STATUS_EXTRACTION_FAILURE\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::sns:publish\",\n            \"Parameters\": {\n                \"TopicArn\": \"arn:aws:sns:us-east-1:245792935030:LOAD_STATUS_EXTRACTION_FAILURE\",\n                \"Message.$\": \"$.Cause\",\n                \"Subject\": \"ODP OPERATIONS DATA EXTRACT SFN FAILED\"\n            },\n            \"End\": true\n        },\n        \"SPOTFIRE_REFRESH_FAILURE\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::sns:publish\",\n            \"Parameters\": {\n                \"TopicArn\": \"arn:aws:sns:us-east-1:245792935030:LOAD_STATUS_EXTRACTION_FAILURE\",\n                \"Message.$\": \"$.Cause\",\n                \"Subject\": \"SPOTFIRE REFRESH FAILED ODP Service Suite Job Load Status\"\n            },\n            \"End\": true\n        }\n    }\n}"