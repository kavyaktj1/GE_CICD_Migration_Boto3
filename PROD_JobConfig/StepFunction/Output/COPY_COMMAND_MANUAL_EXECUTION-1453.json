"{\n    \"Comment\": \"ADHOC Copy Command - source_system, batch_number and re_copy_flag required Arguments\",\n    \"StartAt\": \"COPY_COMMAND_MANUAL_EXECUTION\",\n    \"States\": {\n        \"COPY_COMMAND_MANUAL_EXECUTION\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::glue:startJobRun.sync\",\n            \"ResultPath\": \"$.COPY_RESULT\",\n            \"Parameters\": {\n                \"JobName\": \"job_mrr_redshift_copy\",\n                \"Arguments\": {\n                    \"--source_system\": \"SBOM\",\n                    \"--batch_number\": \"0\",\n                    \"--re_copy_flag\": \"N\"\n                }\n            },\n            \"Retry\": [\n                {\n                    \"ErrorEquals\": [\n                        \"States.Timeout\"\n                    ],\n                    \"MaxAttempts\": 3,\n                    \"IntervalSeconds\": 60,\n                    \"BackoffRate\": 2\n                }\n            ],\n            \"Catch\": [\n                {\n                    \"ErrorEquals\": [\n                        \"States.TaskFailed\"\n                    ],\n                    \"Next\": \"SNS FAILED NOTIFICATION\"\n                }\n            ],\n            \"Next\": \"SNS SUCCESS NOTIFICATION\"\n        },\n        \"SNS FAILED NOTIFICATION\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::sns:publish\",\n            \"Parameters\": {\n                \"TopicArn\": \"arn:aws:sns:us-east-1:245792935030:ODP_SFN_FAILURE\",\n                \"Message.$\": \"$.Cause\",\n                \"Subject\": \" MANUAL COPY COMMAND FAILED\"\n            },\n            \"End\": true\n        },\n        \"SNS SUCCESS NOTIFICATION\": {\n            \"Type\": \"Task\",\n            \"Resource\": \"arn:aws:states:::sns:publish\",\n            \"Parameters\": {\n                \"TopicArn\": \"arn:aws:sns:us-east-1:245792935030:ODP_SFN_SUCCESS\",\n                \"